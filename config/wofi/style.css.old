ndow {
    background-color: #89cff4;
    color: #fcfff4;
}

#outer-box {
    margin: 0px;
    border: 0px solid #89cff4;
    background-color: #282c34;
}

#input {
    font: ComicCode;
    font-size: 16px;
    margin: 5px;
    border:  0px transparent;
    background-color: #282c34;
    border-radius: 0px;
    color: #a3ee8c;
}

#entry {
    color: #a3ee8c;
}

#entry:selected {
    background-color: #89cff4;
}

#text {
    margin: 5px;
    font: ComicCode;
    font-size: 16px;
}

#text:selected {
    color: #282c34;
}
